grammar;

use crate::grammar_ast::{Region, Function};

pub Region: Region = {
    "region" <name:Ident> "{" <funcs:Function*> "}" => Region { name, functions: funcs }
};

Function: Function = {
    "function" <name:Ident> "(" ")" => Function { name , params: Vec::new(), body: Vec::new() //placeholders
           }
};

//will thid
Ident: String = {
    r"[a-zA-Z_][a-zA-Z0-9_]*" => <>.to_string()
};
